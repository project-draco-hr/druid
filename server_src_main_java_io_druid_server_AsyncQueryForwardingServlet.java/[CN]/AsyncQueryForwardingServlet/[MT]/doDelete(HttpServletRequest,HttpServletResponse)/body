{
  final AsyncContext asyncContext=req.startAsync();
  asyncContext.setTimeout(0);
  final HttpResponseHandler<ServletOutputStream,ServletOutputStream> responseHandler=new PassthroughHttpResponseHandler(res);
  final String host=hostFinder.getDefaultHost();
  asyncComplete(res,asyncContext,jsonMapper,routingDruidClient.delete(rewriteURI(host,(HttpServletRequest)asyncContext.getRequest()),responseHandler));
}
