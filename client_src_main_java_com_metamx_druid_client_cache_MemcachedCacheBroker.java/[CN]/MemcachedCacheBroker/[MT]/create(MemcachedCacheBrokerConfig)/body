{
  try {
    return new MemcachedCacheBroker(new MemcachedClient(new ConnectionFactoryBuilder().setProtocol(ConnectionFactoryBuilder.Protocol.BINARY).build(),AddrUtil.getAddresses(config.getHosts())),config.getTimeout(),config.getExpiration());
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
