{
  if (offsets[0] != null) {
    if (offsets[1] != null) {
      int lhs=offsetVals[0];
      int rhs=offsetVals[1];
      if (lhs < rhs) {
        nextOffsetIndex=0;
      }
 else       if (lhs == rhs) {
        nextOffsetIndex=0;
        rollIndexForward(1);
      }
 else {
        nextOffsetIndex=1;
      }
    }
 else {
      nextOffsetIndex=0;
    }
  }
}
