{
  Binders.dataSegmentPullerBinder(binder).addBinding("c*").to(CassandraDataSegmentPuller.class).in(LazySingleton.class);
  PolyBind.optionBinder(binder,Key.get(DataSegmentPusher.class)).addBinding("c*").to(CassandraDataSegmentPusher.class).in(LazySingleton.class);
  JsonConfigProvider.bind(binder,"druid.storage",CassandraDataSegmentConfig.class);
}
