{
  for (Iterator<Map.Entry<String,Integer>> mapIter=removedSegments.entrySet().iterator(); mapIter.hasNext(); ) {
    Map.Entry<String,Integer> segment=mapIter.next();
    int lifetime=segment.getValue() - 1;
    if (lifetime < 0) {
      mapIter.remove();
    }
 else {
      segment.setValue(lifetime);
    }
  }
}
