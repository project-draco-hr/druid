{
  Object val=context.get("useCache");
  if (val == null) {
    return defaultValue;
  }
  if (val instanceof String) {
    return Boolean.parseBoolean((String)val);
  }
 else   if (val instanceof Integer) {
    return (boolean)val;
  }
 else {
    throw new ISE("Unknown type [%s]",val.getClass());
  }
}
