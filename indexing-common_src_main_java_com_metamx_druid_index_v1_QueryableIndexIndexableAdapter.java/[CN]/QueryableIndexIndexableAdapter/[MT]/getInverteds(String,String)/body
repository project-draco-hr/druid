{
  final Column column=input.getColumn(dimension);
  if (column == null) {
    return new EmptyIndexedInts();
  }
  final BitmapIndex bitmaps=column.getBitmapIndex();
  if (bitmaps == null) {
    return new EmptyIndexedInts();
  }
  return new ConciseCompressedIndexedInts(bitmaps.getConciseSet(value));
}
