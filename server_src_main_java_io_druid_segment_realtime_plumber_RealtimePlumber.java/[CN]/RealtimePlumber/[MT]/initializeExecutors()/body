{
  final int maxPendingPersists=config.getMaxPendingPersists();
  if (persistExecutor == null) {
    persistExecutor=Execs.newBlockingSingleThreaded("plumber_persist_%d",maxPendingPersists);
  }
  if (mergeExecutor == null) {
    mergeExecutor=Execs.newBlockingSingleThreaded("plumber_merge_%d",1);
  }
  if (scheduledExecutor == null) {
    scheduledExecutor=Executors.newScheduledThreadPool(1,new ThreadFactoryBuilder().setDaemon(true).setNameFormat("plumber_scheduled_%d").build());
  }
}
