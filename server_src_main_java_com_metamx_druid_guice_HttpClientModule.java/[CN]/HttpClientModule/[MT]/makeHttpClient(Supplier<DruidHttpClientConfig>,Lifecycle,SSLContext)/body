{
  final DruidHttpClientConfig config=configSupplier.get();
  final HttpClientConfig.Builder builder=HttpClientConfig.builder().withNumConnections(config.getNumConnections()).withReadTimeout(config.getReadTimeout());
  if (sslContext != null) {
    builder.withSslContext(sslContext);
  }
  return HttpClientInit.createClient(builder.build(),lifecycle);
}
