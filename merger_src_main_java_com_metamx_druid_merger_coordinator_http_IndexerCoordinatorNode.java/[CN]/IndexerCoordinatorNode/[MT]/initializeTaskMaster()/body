{
  if (taskMaster == null) {
    final ServiceDiscoveryConfig serviceDiscoveryConfig=configFactory.build(ServiceDiscoveryConfig.class);
    taskMaster=new TaskMaster(taskQueue,config,serviceDiscoveryConfig,mergerDBCoordinator,taskRunnerFactory,resourceManagementSchedulerFactory,curatorFramework,emitter);
    lifecycle.addManagedInstance(taskMaster);
  }
}
