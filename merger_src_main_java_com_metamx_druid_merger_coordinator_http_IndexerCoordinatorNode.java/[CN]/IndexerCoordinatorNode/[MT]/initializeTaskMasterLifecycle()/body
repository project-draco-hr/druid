{
  if (taskMasterLifecycle == null) {
    final ServiceDiscoveryConfig serviceDiscoveryConfig=configFactory.build(ServiceDiscoveryConfig.class);
    taskMasterLifecycle=new TaskMasterLifecycle(taskQueue,taskToolboxFactory,config,serviceDiscoveryConfig,taskRunnerFactory,resourceManagementSchedulerFactory,curatorFramework,emitter);
    lifecycle.addManagedInstance(taskMasterLifecycle);
  }
}
