{
  if (taskToolboxFactory == null) {
    final DataSegmentKiller dataSegmentKiller=new S3DataSegmentKiller(s3Service);
    taskToolboxFactory=new TaskToolboxFactory(taskConfig,new LocalTaskActionClientFactory(taskStorage,new TaskActionToolbox(taskQueue,taskLockbox,mergerDBCoordinator,emitter)),emitter,s3Service,segmentPusher,dataSegmentKiller,jsonMapper);
  }
}
