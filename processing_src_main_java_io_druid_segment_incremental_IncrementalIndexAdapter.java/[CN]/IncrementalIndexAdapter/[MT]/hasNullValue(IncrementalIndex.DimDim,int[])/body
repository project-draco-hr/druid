{
  if (dimIndices == null || dimIndices.length == 0) {
    return true;
  }
  for (  int dimIndex : dimIndices) {
    Comparable val=dimDim.getValue(dimIndex);
    if (val == null) {
      return true;
    }
    if (val instanceof String) {
      if (((String)val).length() == 0) {
        return true;
      }
    }
  }
  return false;
}
