{
  Map<String,MutableBitmap> dimInverted=invertedIndexes.get(dimension);
  if (dimInverted == null) {
    return new EmptyIndexedInts();
  }
  final MutableBitmap bitmapIndex=dimInverted.get(value);
  if (bitmapIndex == null) {
    return new EmptyIndexedInts();
  }
  return new BitmapIndexedInts(bitmapIndex);
}
