{
  doSetup();
  Collection<ZkWorker> lazyworkers=remoteTaskRunner.markWorkersLazy(new Predicate<ZkWorker>(){
    @Override public boolean apply(    ZkWorker input){
      return true;
    }
  }
,1);
  Assert.assertEquals(1,lazyworkers.size());
  Assert.assertEquals(1,remoteTaskRunner.getLazyWorkers().size());
}
