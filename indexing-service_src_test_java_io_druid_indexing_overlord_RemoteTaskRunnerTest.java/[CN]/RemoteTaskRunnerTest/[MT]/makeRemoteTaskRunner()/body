{
  RemoteTaskRunnerConfig config=new TestRemoteTaskRunnerConfig();
  remoteTaskRunner=new RemoteTaskRunner(jsonMapper,config,new IndexerZkConfig().setZkPathsConfig(new ZkPathsConfig(){
