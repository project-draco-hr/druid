{
  super(String.format("index_hadoop_%s_interval_%s",config.getDataSource(),new DateTime()),config.getDataSource(),JodaUtils.umbrellaInterval(config.getIntervals()));
  if (config.isUpdaterJobSpecSet()) {
    throw new IllegalArgumentException("The UpDaterJobSpec field of the Hadoop Druid indexer config must be set to null ");
  }
  this.config=config;
}
