{
  Preconditions.checkArgument(dimensionCompression == null || dimensionCompression.equals(UNCOMPRESSED) || COMPRESSION_NAMES.contains(dimensionCompression),"Unknown compression type[%s]",dimensionCompression);
  Preconditions.checkArgument(metricCompression == null || COMPRESSION_NAMES.contains(metricCompression),"Unknown compression type[%s]",metricCompression);
  this.bitmapSerdeFactory=bitmapSerdeFactory != null ? bitmapSerdeFactory : IndexIO.CONFIGURED_BITMAP_SERDE_FACTORY;
  this.metricCompression=metricCompression;
  this.dimensionCompression=dimensionCompression;
}
