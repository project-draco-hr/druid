{
  try {
    final Server instance=selector.pick();
    if (instance == null) {
      throw new ISE("Cannot find instance of indexingService");
    }
    return String.format("http://%s:%s/druid/indexer/v1",instance.getHost(),instance.getPort());
  }
 catch (  Exception e) {
    throw Throwables.propagate(e);
  }
}
