{
  if (conglomerate == null) {
    StupidPool<ByteBuffer> computationBufferPool=ServerInit.makeComputeScratchPool(PropUtils.getPropertyAsInt(props,"druid.computation.buffer.size",1024 * 1024 * 1024));
    conglomerate=new DefaultQueryRunnerFactoryConglomerate(ServerInit.initDefaultQueryTypes(configFactory,computationBufferPool));
  }
}
