{
  if (metadataUpdater == null) {
    metadataUpdater=new MetadataUpdater(getJsonMapper(),getConfigFactory().build(MetadataUpdaterConfig.class),getPhoneBook(),new DbConnector(getConfigFactory().build(DbConnectorConfig.class)).getDBI());
    getLifecycle().addManagedInstance(metadataUpdater);
  }
}
