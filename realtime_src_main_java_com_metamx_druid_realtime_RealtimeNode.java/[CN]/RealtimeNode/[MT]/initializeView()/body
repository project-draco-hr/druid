{
  if (view == null) {
    final MutableServerView view=new OnlyNewSegmentWatcherServerView();
    final ClientInventoryManager clientInventoryManager=new ClientInventoryManager(getConfigFactory().build(ClientConfig.class),getPhoneBook(),view);
    getLifecycle().addManagedInstance(clientInventoryManager);
    this.view=view;
  }
}
