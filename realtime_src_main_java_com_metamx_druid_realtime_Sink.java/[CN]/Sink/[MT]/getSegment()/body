{
  return new DataSegment(schema.getDataSource(),interval,interval.getStart().toString(),ImmutableMap.<String,Object>of(),Lists.<String>newArrayList(),Lists.transform(Arrays.asList(schema.getAggregators()),new Function<AggregatorFactory,String>(){
    @Override public String apply(    @Nullable AggregatorFactory input){
      return input.getName();
    }
  }
),schema.getShardSpec(),0);
}
