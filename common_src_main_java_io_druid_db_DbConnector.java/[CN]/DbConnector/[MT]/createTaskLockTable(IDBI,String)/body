{
  createTable(dbi,taskLocksTableName,String.format(isPostgreSQL(dbi) ? "CREATE TABLE %1$s (\n" + "  id bigserial NOT NULL,\n" + "  task_id varchar(255) DEFAULT NULL,\n"+ "  lock_payload bytea,\n"+ "  PRIMARY KEY (id)\n"+ ");\n"+ "CREATE INDEX ON %1$s(task_id);" : "CREATE TABLE `%s` (\n" + "  `id` bigint(20) NOT NULL AUTO_INCREMENT,\n" + "  `task_id` varchar(255) DEFAULT NULL,\n"+ "  `lock_payload` longblob,\n"+ "  PRIMARY KEY (`id`),\n"+ "  KEY `task_id` (`task_id`)\n"+ ")",taskLocksTableName));
}
