{
  Preconditions.checkArgument(allowRegularTasks || runnable instanceof PrioritizedRunnable,"task does not implement PrioritizedRunnable");
  return PrioritizedListenableFutureTask.create(ListenableFutureTask.create(runnable,value),runnable instanceof PrioritizedRunnable ? ((PrioritizedRunnable)runnable).getPriority() : defaultPriority);
}
