{
  Preconditions.checkArgument(allowRegularTasks || callable instanceof PrioritizedCallable,"task does not implement PrioritizedCallable");
  return PrioritizedListenableFutureTask.create(ListenableFutureTask.create(callable),callable instanceof PrioritizedCallable ? ((PrioritizedCallable)callable).getPriority() : defaultPriority);
}
