{
  return ImmutableConciseSet.union(FunctionalIterable.create(selector.getDimensionValues(dimension)).filter(predicate).transform(new Function<String,ImmutableConciseSet>(){
    @Override public ImmutableConciseSet apply(    @Nullable String input){
      return selector.getConciseInvertedIndex(dimension,input);
    }
  }
));
}
