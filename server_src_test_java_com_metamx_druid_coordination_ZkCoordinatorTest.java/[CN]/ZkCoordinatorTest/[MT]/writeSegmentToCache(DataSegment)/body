{
  if (!cacheDir.exists()) {
    cacheDir.mkdir();
  }
  File segmentInfoCacheFile=new File(cacheDir,segment.getIdentifier());
  try {
    jsonMapper.writeValue(segmentInfoCacheFile,segment);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  Assert.assertTrue(segmentInfoCacheFile.exists());
}
