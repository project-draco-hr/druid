{
  int numMinutes=0;
  for (  Interval interval : query.getIntervals()) {
    numMinutes+=Minutes.minutesIn(interval).getMinutes();
  }
  return new ServiceMetricEvent.Builder().setUser2(query.getDataSource().getNames().toArray(new String[query.getDataSource().getNames().size()])).setUser4(query.getType()).setUser5(Lists.transform(query.getIntervals(),new Function<Interval,String>(){
    @Override public String apply(    Interval input){
      return input.toString();
    }
  }
).toArray(new String[query.getIntervals().size()])).setUser6(String.valueOf(query.hasFilters())).setUser9(Minutes.minutes(numMinutes).toString());
}
