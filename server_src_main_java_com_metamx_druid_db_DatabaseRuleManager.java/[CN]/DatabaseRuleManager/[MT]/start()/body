{
synchronized (lock) {
    if (started) {
      return;
    }
    createDefaultRule(dbi,getRulesTable(),config.get().getDefaultTier(),jsonMapper);
    ScheduledExecutors.scheduleWithFixedDelay(exec,new Duration(0),config.get().getPollDuration().toStandardDuration(),new Runnable(){
      @Override public void run(){
        poll();
      }
    }
);
    started=true;
  }
}
