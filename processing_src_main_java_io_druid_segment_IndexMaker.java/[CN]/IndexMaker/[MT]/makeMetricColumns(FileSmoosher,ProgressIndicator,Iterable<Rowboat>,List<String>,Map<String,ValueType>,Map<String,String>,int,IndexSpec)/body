{
  final String metSection="make metric columns";
  progress.startSection(metSection);
  int metIndex=0;
  for (  String metric : mergedMetrics) {
    makeMetricColumn(v9Smoosher,progress,theRows,metIndex,metric,valueTypes,metricTypeNames,rowCount,indexSpec.getMetricCompressionStrategy());
    metIndex++;
  }
  progress.stopSection(metSection);
}
