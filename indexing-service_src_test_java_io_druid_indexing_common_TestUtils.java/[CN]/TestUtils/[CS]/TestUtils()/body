{
  jsonMapper=new DefaultObjectMapper();
  indexIO=new IndexIO(jsonMapper,new ColumnConfig(){
    @Override public int columnCacheSizeBytes(){
      return 0;
    }
  }
);
  indexMerger=new IndexMerger(jsonMapper,indexIO);
  indexMaker=new IndexMaker(jsonMapper,indexIO);
  final List<? extends Module> list=new ServerModule().getJacksonModules();
  for (  Module module : list) {
    jsonMapper.registerModule(module);
  }
  jsonMapper.setInjectableValues(new InjectableValues.Std().addValue(IndexIO.class,indexIO).addValue(IndexMerger.class,indexMerger).addValue(IndexMaker.class,indexMaker).addValue(ObjectMapper.class,jsonMapper));
}
