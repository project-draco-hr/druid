{
  QueryToolChest toolChest=factory.getToolchest();
  QueryRunner theRunner=new FinalizeResultsQueryRunner<>(toolChest.mergeResults(toolChest.preMergeQueryDecoration(runner)),toolChest);
  HashMap<String,Object> context=new HashMap<String,Object>();
  Sequence<Row> queryResult=theRunner.run(query,context);
  return Sequences.toList(queryResult,Lists.<Row>newArrayList());
}
