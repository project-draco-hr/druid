{
  this.dimension=dimension;
  if (predicate == null) {
    this.predicate=null;
  }
 else {
    this.predicate=extractionFn == null ? predicate : new Predicate<String>(){
      @Override public boolean apply(      String input){
        return predicate.apply(extractionFn.apply(input));
      }
    }
;
  }
}
