{
  long now=System.currentTimeMillis();
  for (  TaskWrapper pendingTask : pendingTasks) {
    if (new Duration(pendingTask.getCreatedTime(),now).isEqual(config.getMaxPendingTaskDuration()) || new Duration(pendingTask.getCreatedTime(),now).isLongerThan(config.getMaxPendingTaskDuration())) {
      return true;
    }
  }
  return false;
}
