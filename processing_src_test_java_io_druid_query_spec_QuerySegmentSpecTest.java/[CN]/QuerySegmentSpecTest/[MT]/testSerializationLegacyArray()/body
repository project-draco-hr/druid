{
  QuerySegmentSpec spec=jsonMapper.readValue("[\"2011-09-01/2011-10-10\", \"2011-11-01/2011-11-10\"]",QuerySegmentSpec.class);
  Assert.assertTrue(spec instanceof LegacySegmentSpec);
  Assert.assertEquals(ImmutableList.of(new Interval("2011-09-01/2011-10-10"),new Interval("2011-11-01/2011-11-10")),spec.getIntervals());
}
