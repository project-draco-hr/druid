{
  DateTime now=new DateTime();
  PeriodLoadRule rule=new PeriodLoadRule(new Period("P1M"),0,"");
  Assert.assertTrue(rule.appliesTo(builder.interval(new Interval(now.minusWeeks(1),now)).build()));
  Assert.assertTrue(rule.appliesTo(builder.interval(new Interval(now.minusDays(1),now.plusDays(1))).build()));
  Assert.assertFalse(rule.appliesTo(builder.interval(new Interval(now.plusDays(1),now.plusDays(2))).build()));
}
