{
  byte[] dimensionBytes=StringUtils.toUtf8(dimension);
  byte[] valueBytes=StringUtils.toUtf8(value);
  return ByteBuffer.allocate(2 + dimensionBytes.length + valueBytes.length).put(DimFilterCacheHelper.EXTRACTION_CACHE_ID).put(dimensionBytes).put(DimFilterCacheHelper.STRING_SEPARATOR).put(valueBytes).array();
}
