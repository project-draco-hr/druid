{
  log.info("Performing action for task[%s]: %s",task.getId(),taskAction);
  final RetType ret=taskAction.perform(task,toolbox);
  try {
    storage.addAuditLog(task,taskAction);
  }
 catch (  Exception e) {
    log.makeAlert(e,"Failed to record action in audit log").addData("task",task.getId()).addData("actionClass",taskAction.getClass().getName()).emit();
  }
  return ret;
}
