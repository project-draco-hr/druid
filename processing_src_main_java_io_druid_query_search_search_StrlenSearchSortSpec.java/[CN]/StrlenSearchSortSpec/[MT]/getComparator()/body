{
  return new Comparator<SearchHit>(){
    @Override public int compare(    SearchHit s,    SearchHit s1){
      int res=StringComparators.STRLEN.compare(s.getValue(),s1.getValue());
      if (res == 0) {
        res=StringComparators.LEXICOGRAPHIC.compare(s.getDimension(),s1.getDimension());
      }
      return res;
    }
  }
;
}
