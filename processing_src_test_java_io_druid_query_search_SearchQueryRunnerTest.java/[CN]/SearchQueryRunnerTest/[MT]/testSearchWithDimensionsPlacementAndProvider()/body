{
  Map<String,Set<String>> expectedResults=Maps.newTreeMap(String.CASE_INSENSITIVE_ORDER);
  expectedResults.put(QueryRunnerTestHelper.marketDimension,new HashSet<String>(Arrays.asList("total_market")));
  checkSearchQuery(Druids.newSearchQueryBuilder().dataSource(QueryRunnerTestHelper.dataSource).granularity(QueryRunnerTestHelper.allGran).dimensions(Arrays.asList(QueryRunnerTestHelper.placementishDimension,QueryRunnerTestHelper.marketDimension)).intervals(QueryRunnerTestHelper.fullOnInterval).query("mark").build(),expectedResults);
}
