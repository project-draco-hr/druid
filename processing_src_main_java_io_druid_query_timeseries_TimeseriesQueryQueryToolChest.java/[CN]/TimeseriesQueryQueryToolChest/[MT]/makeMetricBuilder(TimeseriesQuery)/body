{
  int numMinutes=0;
  for (  Interval interval : query.getIntervals()) {
    numMinutes+=Minutes.minutesIn(interval).getMinutes();
  }
  return new ServiceMetricEvent.Builder().setUser2(query.getDataSource().toString()).setUser4("timeseries").setUser5(COMMA_JOIN.join(query.getIntervals())).setUser6(String.valueOf(query.hasFilters())).setUser7(String.format("%,d aggs",query.getAggregatorSpecs().size())).setUser9(Minutes.minutes(numMinutes).toString()).setUser10(query.getId());
}
