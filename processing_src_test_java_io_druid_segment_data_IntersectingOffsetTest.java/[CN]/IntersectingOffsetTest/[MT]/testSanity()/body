{
  assertExpected(new int[]{2,3,6,7},new IntersectingOffset(new ArrayBasedOffset(new int[]{1,2,3,6,7,8}),new ArrayBasedOffset(new int[]{2,3,4,5,6,7})));
  assertExpected(new int[]{2,3,6,7},new IntersectingOffset(new ArrayBasedOffset(new int[]{2,3,4,5,6,7}),new ArrayBasedOffset(new int[]{1,2,3,6,7,8})));
  assertExpected(new int[]{},new IntersectingOffset(new ArrayBasedOffset(new int[]{1,2,3,6,7,8}),new ArrayBasedOffset(new int[]{4,5,9,10})));
  assertExpected(new int[]{},new IntersectingOffset(new ArrayBasedOffset(new int[]{4,5,9,10}),new ArrayBasedOffset(new int[]{1,2,3,6,7,8})));
  assertExpected(new int[]{},new IntersectingOffset(new ArrayBasedOffset(new int[]{1,2,3,6,7,8}),new ArrayBasedOffset(new int[]{})));
  assertExpected(new int[]{},new IntersectingOffset(new ArrayBasedOffset(new int[]{}),new ArrayBasedOffset(new int[]{1,2,3,6,7,8})));
}
