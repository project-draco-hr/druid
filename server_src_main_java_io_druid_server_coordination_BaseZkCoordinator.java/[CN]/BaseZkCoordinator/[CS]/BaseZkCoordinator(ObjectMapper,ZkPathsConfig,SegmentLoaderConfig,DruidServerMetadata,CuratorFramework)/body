{
  this.jsonMapper=jsonMapper;
  this.zkPaths=zkPaths;
  this.config=config;
  this.me=me;
  this.curator=curator;
  this.loadingExec=MoreExecutors.listeningDecorator(Executors.newFixedThreadPool(config.getNumLoadingThreads(),new ThreadFactoryBuilder().setDaemon(true).setNameFormat("ZkCoordinator-%s").build()));
}
