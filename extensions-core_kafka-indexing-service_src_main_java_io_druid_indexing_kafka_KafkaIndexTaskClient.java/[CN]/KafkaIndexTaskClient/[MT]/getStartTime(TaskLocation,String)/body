{
  try {
    final URI serviceUri=new URI("http",null,location.getHost(),location.getPort(),String.format("%s/%s/%s",BASE_PATH,id,"time/start"),null,null);
    final StatusResponseHolder response=submitRequest(new Request(HttpMethod.GET,serviceUri.toURL()),true);
    return response.getContent() == null || response.getContent().isEmpty() ? null : jsonMapper.readValue(response.getContent(),DateTime.class);
  }
 catch (  Exception e) {
    throw Throwables.propagate(e);
  }
}
