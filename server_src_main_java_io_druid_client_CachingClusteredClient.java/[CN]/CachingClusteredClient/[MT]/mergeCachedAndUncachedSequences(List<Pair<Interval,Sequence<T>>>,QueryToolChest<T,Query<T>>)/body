{
  if (sequencesByInterval.isEmpty()) {
    return Sequences.empty();
  }
  return toolChest.mergeSequencesUnordered(Sequences.simple(Lists.transform(sequencesByInterval,new Function<Pair<Interval,Sequence<T>>,Sequence<T>>(){
    @Override public Sequence<T> apply(    Pair<Interval,Sequence<T>> input){
      return input.rhs;
    }
  }
)));
}
