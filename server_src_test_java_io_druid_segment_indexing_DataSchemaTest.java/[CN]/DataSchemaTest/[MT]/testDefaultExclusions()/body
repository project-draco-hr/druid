{
  DataSchema schema=new DataSchema("test",new StringInputRowParser(new JSONParseSpec(new TimestampSpec("time","auto",null),new DimensionsSpec(ImmutableList.of("dimB","dimA"),null,null))),new AggregatorFactory[]{new DoubleSumAggregatorFactory("metric1","col1"),new DoubleSumAggregatorFactory("metric2","col2")},new ArbitraryGranularitySpec(QueryGranularity.DAY,ImmutableList.of(Interval.parse("2014/2015"))));
  Assert.assertEquals(ImmutableSet.of("time","col1","col2"),schema.getParser().getParseSpec().getDimensionsSpec().getDimensionExclusions());
}
