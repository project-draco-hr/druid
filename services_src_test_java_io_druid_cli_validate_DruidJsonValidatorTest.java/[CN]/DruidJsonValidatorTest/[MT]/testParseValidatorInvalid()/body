{
  String type="parse";
  Cli<?> parser=Cli.builder("validator").withCommand(DruidJsonValidator.class).build();
  Object command=parser.parse("validator","-f","simple_test_data_record_parser_invalid.json","-t",type);
  Assert.assertNotNull(command);
  DruidJsonValidator druidJsonValidator=(DruidJsonValidator)command;
  druidJsonValidator.run();
}
