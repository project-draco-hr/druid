{
  final byte[][] valuesBytes=new byte[values.size()][];
  int valuesBytesSize=0;
  int index=0;
  for (  String value : values) {
    valuesBytes[index]=StringUtils.toUtf8(value);
    valuesBytesSize+=valuesBytes[index].length;
    ++index;
  }
  final ByteBuffer queryCacheKey=ByteBuffer.allocate(2 + valuesBytesSize).put(caseSensitive ? (byte)1 : 0).put(CACHE_TYPE_ID);
  for (  byte[] bytes : valuesBytes) {
    queryCacheKey.put(bytes);
  }
  return queryCacheKey.array();
}
