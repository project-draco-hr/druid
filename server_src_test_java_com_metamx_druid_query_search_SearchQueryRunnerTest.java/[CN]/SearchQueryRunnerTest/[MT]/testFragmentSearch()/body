{
  SearchQuery searchQuery=Druids.newSearchQueryBuilder().dataSource(QueryRunnerTestHelper.dataSource).granularity(QueryRunnerTestHelper.allGran).intervals(QueryRunnerTestHelper.fullOnInterval).query(new FragmentSearchQuerySpec(Arrays.asList("auto","ve"),null)).build();
  Map<String,Set<String>> expectedResults=new HashMap<String,Set<String>>();
  expectedResults.put(QueryRunnerTestHelper.qualityDimension,Sets.newHashSet("automotive"));
  checkSearchQuery(searchQuery,expectedResults);
}
