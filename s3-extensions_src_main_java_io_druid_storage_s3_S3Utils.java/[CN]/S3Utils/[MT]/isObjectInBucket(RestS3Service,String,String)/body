{
  try {
    s3Client.getObjectDetails(new S3Bucket(bucketName),objectKey);
  }
 catch (  S3ServiceException e) {
    if (404 == e.getResponseCode() || "NoSuchKey".equals(e.getS3ErrorCode()) || "NoSuchBucket".equals(e.getS3ErrorCode())) {
      return false;
    }
    if ("AccessDenied".equals(e.getS3ErrorCode())) {
      return true;
    }
    throw e;
  }
  return true;
}
