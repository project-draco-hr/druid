{
  final IncrementalIndex<Aggregator> incrementalIndex=new OnheapIncrementalIndex(new IncrementalIndexSchema.Builder().withQueryGranularity(QueryGranularity.NONE).withMetrics(new AggregatorFactory[]{new CountAggregatorFactory("count")}).withDimensionsSpec(new DimensionsSpec(Arrays.asList("dim0","dim1"),null,null)).build(),true,1000000);
  closer.closeLater(incrementalIndex);
  Assert.assertEquals(Arrays.asList("dim0","dim1"),incrementalIndex.getDimensionNames());
}
