{
  return Arrays.asList(new Object[][]{{new IndexCreator(){
    @Override public IncrementalIndex createIndex(    AggregatorFactory[] factories){
      return IncrementalIndexTest.createIndex(true,factories);
    }
  }
},{new IndexCreator(){
    @Override public IncrementalIndex createIndex(    AggregatorFactory[] factories){
      return IncrementalIndexTest.createIndex(false,factories);
    }
  }
}});
}
