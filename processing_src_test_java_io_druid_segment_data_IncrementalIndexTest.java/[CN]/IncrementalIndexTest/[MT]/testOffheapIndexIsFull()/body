{
  OffheapIncrementalIndex index=new OffheapIncrementalIndex(0L,QueryGranularity.NONE,new AggregatorFactory[]{new CountAggregatorFactory("count")},TestQueryRunners.pool,true,(10 + 2) * 1024 * 1024);
  int rowCount=0;
  for (int i=0; i < 500; i++) {
    rowCount=index.add(getRow(System.currentTimeMillis(),i,100));
    if (!index.canAppendRow()) {
      break;
    }
  }
  Assert.assertTrue("rowCount : " + rowCount,rowCount > 200 && rowCount < 600);
}
