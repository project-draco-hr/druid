{
  if (offheap) {
    return new OffheapIncrementalIndex(0L,QueryGranularity.NONE,new AggregatorFactory[]{new CountAggregatorFactory("count")},TestQueryRunners.pool,true);
  }
 else {
    return new OnheapIncrementalIndex(0L,QueryGranularity.NONE,new AggregatorFactory[]{new CountAggregatorFactory("count")});
  }
}
