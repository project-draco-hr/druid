{
  Object singleVal;
  if (dimVal instanceof List) {
    List dimValList=(List)dimVal;
    singleVal=dimValList.size() == 0 ? null : dimValList.get(0);
  }
 else {
    singleVal=dimVal;
  }
  if (singleVal == null) {
    return null;
  }
  return TYPE_MAP.get(singleVal.getClass().getSimpleName());
}
