{
  return "TimeAndDims{" + "timestamp=" + new DateTime(timestamp) + ", dims="+ Lists.transform(Arrays.asList(dims),new Function<int[],Object>(){
    @Override public Object apply(    @Nullable int[] input){
      if (input == null || input.length == 0) {
        return Arrays.asList("null");
      }
      return Arrays.asList(input);
    }
  }
)+ '}';
}
