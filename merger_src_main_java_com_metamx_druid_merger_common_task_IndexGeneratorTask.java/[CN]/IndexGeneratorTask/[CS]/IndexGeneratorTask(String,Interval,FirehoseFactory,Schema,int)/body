{
  super(String.format("%s_generator_%s_%s_%s",groupId,interval.getStart(),interval.getEnd(),schema.getShardSpec().getPartitionNum()),groupId,schema.getDataSource(),Preconditions.checkNotNull(interval,"interval"));
  this.firehoseFactory=firehoseFactory;
  this.schema=schema;
  this.rowFlushBoundary=rowFlushBoundary;
}
