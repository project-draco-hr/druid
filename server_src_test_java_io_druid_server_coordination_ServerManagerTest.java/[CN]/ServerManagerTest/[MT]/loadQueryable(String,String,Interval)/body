{
  try {
    serverManager.loadSegment(new DataSegment(dataSource,interval,version,ImmutableMap.<String,Object>of("version",version,"interval",interval),Arrays.asList("dim1","dim2","dim3"),Arrays.asList("metric1","metric2"),NoneShardSpec.instance(),IndexIO.CURRENT_VERSION_ID,123L));
