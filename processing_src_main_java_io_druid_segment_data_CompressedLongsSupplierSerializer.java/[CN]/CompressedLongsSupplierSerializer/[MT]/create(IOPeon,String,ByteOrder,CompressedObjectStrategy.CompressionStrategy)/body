{
  final int sizePer=0xFFFF / Longs.BYTES;
  final CompressedLongsSupplierSerializer retVal=new CompressedLongsSupplierSerializer(sizePer,new GenericIndexedWriter<ResourceHolder<LongBuffer>>(ioPeon,filenameBase,CompressedLongBufferObjectStrategy.getBufferForOrder(order,compression,sizePer)),compression);
  return retVal;
}
