{
  final CompressedLongsSupplierSerializer retVal=new CompressedLongsSupplierSerializer(CompressedLongsIndexedSupplier.MAX_LONGS_IN_BUFFER,new GenericIndexedWriter<ResourceHolder<LongBuffer>>(ioPeon,filenameBase,CompressedLongBufferObjectStrategy.getBufferForOrder(order,compression,CompressedLongsIndexedSupplier.MAX_LONGS_IN_BUFFER)),compression);
  return retVal;
}
