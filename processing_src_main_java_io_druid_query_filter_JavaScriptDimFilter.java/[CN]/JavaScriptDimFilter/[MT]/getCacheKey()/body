{
  final byte[] dimensionBytes=dimension.getBytes(Charsets.UTF_8);
  final byte[] functionBytes=function.getBytes(Charsets.UTF_8);
  return ByteBuffer.allocate(1 + dimensionBytes.length + functionBytes.length).put(DimFilterCacheHelper.JAVASCRIPT_CACHE_ID).put(dimensionBytes).put(functionBytes).array();
}
