{
  final HadoopIngestionSchema schema;
  try {
    schema=jsonReadWriteRead("{\"cleanupOnFailure\":false}",HadoopIngestionSchema.class);
  }
 catch (  Exception e) {
    throw Throwables.propagate(e);
  }
  Assert.assertEquals("cleanupOnFailure",schema.getDriverConfig().isCleanupOnFailure(),false);
}
