{
  final HadoopIngestionSchema schema;
  try {
    schema=jsonReadWriteRead("{}",HadoopIngestionSchema.class);
  }
 catch (  Exception e) {
    throw Throwables.propagate(e);
  }
  Assert.assertEquals("cleanupOnFailure",schema.getDriverConfig().isCleanupOnFailure(),true);
  Assert.assertEquals("overwriteFiles",schema.getDriverConfig().isOverwriteFiles(),false);
  Assert.assertEquals("isDeterminingPartitions",schema.getDriverConfig().getPartitionsSpec().isDeterminingPartitions(),false);
}
