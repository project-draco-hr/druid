{
  return new Comparator<SearchHit>(){
    @Override public int compare(    SearchHit searchHit1,    SearchHit searchHit2){
      int retVal=StringComparators.ALPHANUMERIC.compare(searchHit1.getValue(),searchHit2.getValue());
      if (retVal == 0) {
        retVal=StringComparators.LEXICOGRAPHIC.compare(searchHit1.getDimension(),searchHit2.getDimension());
      }
      return retVal;
    }
  }
;
}
