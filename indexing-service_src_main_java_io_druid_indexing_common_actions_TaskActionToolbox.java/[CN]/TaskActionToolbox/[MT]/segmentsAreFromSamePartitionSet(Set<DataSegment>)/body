{
  Preconditions.checkArgument(!segments.isEmpty(),"segments nonempty");
  final DataSegment firstSegment=segments.iterator().next();
  for (  final DataSegment segment : segments) {
    if (!segment.getDataSource().equals(firstSegment.getDataSource()) || !segment.getInterval().equals(firstSegment.getInterval()) || !segment.getVersion().equals(firstSegment.getVersion())) {
      return false;
    }
  }
  return true;
}
