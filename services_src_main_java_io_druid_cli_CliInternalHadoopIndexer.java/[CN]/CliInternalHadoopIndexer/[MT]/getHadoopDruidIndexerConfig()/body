{
  if (config == null) {
    try {
      if (argumentSpec.startsWith("{")) {
        config=HadoopDruidIndexerConfig.fromString(argumentSpec);
      }
 else {
        config=HadoopDruidIndexerConfig.fromFile(new File(argumentSpec));
      }
    }
 catch (    Exception e) {
      throw Throwables.propagate(e);
    }
  }
  return config;
}
