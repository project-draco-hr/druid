{
  CountAggregator agg=new CountAggregator("billy");
  Assert.assertEquals("billy",agg.getName());
  Assert.assertEquals(0l,agg.get());
  Assert.assertEquals(0l,agg.get());
  Assert.assertEquals(0l,agg.get());
  agg.aggregate();
  Assert.assertEquals(1l,agg.get());
  Assert.assertEquals(1l,agg.get());
  Assert.assertEquals(1l,agg.get());
  agg.aggregate();
  Assert.assertEquals(2l,agg.get());
  Assert.assertEquals(2l,agg.get());
  Assert.assertEquals(2l,agg.get());
}
