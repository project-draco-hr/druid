{
  if (args.length == 0) {
    new HelpFormatter().printHelp("<java invocation>",opts);
    return null;
  }
  CommandLine cli=null;
  try {
    cli=new PosixParser().parse(opts,args,false);
  }
 catch (  ParseException e) {
    System.out.println(e.getMessage());
    new HelpFormatter().printHelp("<java invocation>",opts);
    return null;
  }
  if (cli.hasOption("help")) {
    new HelpFormatter().printHelp("<java invocation>",opts);
    return null;
  }
  return cli;
}
