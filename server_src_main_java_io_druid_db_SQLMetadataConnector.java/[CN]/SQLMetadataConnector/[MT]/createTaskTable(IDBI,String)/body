{
  createTable(dbi,tableName,String.format("CREATE TABLE %s (id VARCHAR(255) NOT NULL, created_date VARCHAR(255) NOT NULL, " + "datasource VARCHAR(255) NOT NULL, payload CLOB NOT NULL, status_payload CLOB NOT NULL, " + "active SMALLINT NOT NULL DEFAULT 0, PRIMARY KEY (id))",tableName));
  createIndex(dbi,tableName,"task_active_created_date","active, created_date");
}
