{
  createTable(dbi,tableName,String.format("CREATE TABLE %1$s (\n" + "  id VARCHAR(255) NOT NULL,\n" + "  dataSource VARCHAR(255) NOT NULL,\n"+ "  version VARCHAR(255) NOT NULL,\n"+ "  payload %2$s NOT NULL,\n"+ "  PRIMARY KEY (id)\n"+ ");\n"+ "CREATE INDEX idx_%1$s_datasource ON %1$s(dataSource);",tableName,getPayloadType()));
  createIndex(dbi,tableName,"rules_dataSource","dataSource");
}
