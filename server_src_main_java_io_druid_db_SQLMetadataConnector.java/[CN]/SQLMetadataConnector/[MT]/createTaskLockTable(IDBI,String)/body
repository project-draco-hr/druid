{
  createTable(dbi,tableName,ImmutableList.of(String.format("CREATE TABLE %1$s (\n" + "  id %2$s NOT NULL,\n" + "  task_id VARCHAR(255) DEFAULT NULL,\n"+ "  lock_payload %3$s,\n"+ "  PRIMARY KEY (id)\n"+ ")",tableName,getSerialType(),getPayloadType()),String.format("CREATE INDEX idx_%1$s_task_id ON %1$s(task_id)",tableName)));
}
