{
  createTable(dbi,tableName,ImmutableList.of(String.format("CREATE TABLE %1$s (\n" + "  id %2$s NOT NULL,\n" + "  %4$s_id VARCHAR(255) DEFAULT NULL,\n"+ "  lock_payload %3$s,\n"+ "  PRIMARY KEY (id)\n"+ ")",tableName,getSerialType(),getPayloadType(),entryTypeName),String.format("CREATE INDEX idx_%1$s_%2$s_id ON %1$s(%2$s_id)",tableName,entryTypeName)));
}
