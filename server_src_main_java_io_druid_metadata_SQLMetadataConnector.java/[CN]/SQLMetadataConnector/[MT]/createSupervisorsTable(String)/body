{
  createTable(tableName,ImmutableList.of(String.format("CREATE TABLE %1$s (\n" + "  id VARCHAR(255) NOT NULL,\n" + "  spec_id VARCHAR(255) NOT NULL,\n"+ "  version VARCHAR(255) NOT NULL,\n"+ "  payload %2$s NOT NULL,\n"+ "  PRIMARY KEY (id)\n"+ ")",tableName,getPayloadType()),String.format("CREATE INDEX idx_%1$s_spec_id ON %1$s(spec_id)",tableName)));
}
