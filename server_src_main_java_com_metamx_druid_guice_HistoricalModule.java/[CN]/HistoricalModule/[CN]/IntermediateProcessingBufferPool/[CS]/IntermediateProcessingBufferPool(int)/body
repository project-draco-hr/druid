{
  super(new Supplier<ByteBuffer>(){
    final AtomicLong count=new AtomicLong(0);
    @Override public ByteBuffer get(){
      log.info("Allocating new intermediate processing buffer[%,d] of size[%,d]",count.getAndIncrement(),computationBufferSize);
      return ByteBuffer.allocateDirect(computationBufferSize);
    }
  }
);
}
