{
  List<FireChief> chiefs=this.chiefs.get(datasource);
  if (chiefs == null) {
    return null;
  }
  FireDepartmentMetrics snapshot=null;
  for (  FireChief chief : chiefs) {
    if (snapshot == null) {
      snapshot=chief.getMetrics().snapshot();
    }
 else {
      snapshot.merge(chief.getMetrics());
    }
  }
  return snapshot;
}
