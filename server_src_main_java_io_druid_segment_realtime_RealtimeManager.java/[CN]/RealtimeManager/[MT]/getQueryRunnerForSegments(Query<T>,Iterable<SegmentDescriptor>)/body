{
  QueryRunnerFactory<T,Query<T>> factory=conglomerate.findFactory(query);
  Iterable<FireChief> chiefsOfDataSource=chiefs.get(getDataSourceName(query));
  return chiefsOfDataSource == null ? new NoopQueryRunner<T>() : factory.getToolchest().mergeResults(factory.mergeRunners(Executors.newSingleThreadExecutor(),Iterables.transform(chiefsOfDataSource,new Function<FireChief,QueryRunner<T>>(){
    @Override public QueryRunner<T> apply(    FireChief input){
      return input.getQueryRunner(query);
    }
  }
)));
}
