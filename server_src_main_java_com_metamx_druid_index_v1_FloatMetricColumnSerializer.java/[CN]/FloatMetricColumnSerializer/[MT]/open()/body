{
  littleMetricsWriter=CompressedFloatsSupplierSerializer.create(ioPeon,String.format("%s_little",metricName),ByteOrder.LITTLE_ENDIAN);
  bigEndianMetricsWriter=CompressedFloatsSupplierSerializer.create(ioPeon,String.format("%s_big",metricName),ByteOrder.BIG_ENDIAN);
  littleMetricsWriter.open();
  bigEndianMetricsWriter.open();
}
