{
  return new ServiceMetricEvent.Builder().setUser2(DataSourceUtil.getMetricName(query.getDataSource())).setUser4(query.getType()).setUser5(Lists.transform(query.getIntervals(),new Function<Interval,String>(){
    @Override public String apply(    Interval input){
      return input.toString();
    }
  }
).toArray(new String[query.getIntervals().size()])).setUser6(String.valueOf(query.hasFilters())).setUser9(query.getDuration().toPeriod().toStandardMinutes().toString());
}
