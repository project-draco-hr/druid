{
  try {
    LogLevelAdjuster.register();
    final Lifecycle lifecycle=getInjector().getInstance(Lifecycle.class);
    try {
      lifecycle.start();
    }
 catch (    Throwable t) {
      log.error(t,"Error when starting up.  Failing.");
      System.exit(1);
    }
    lifecycle.join();
  }
 catch (  Exception e) {
    throw Throwables.propagate(e);
  }
}
