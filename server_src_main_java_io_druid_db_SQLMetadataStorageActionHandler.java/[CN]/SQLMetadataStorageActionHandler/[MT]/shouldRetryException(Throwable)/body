{
  return e != null && (e instanceof SQLTransientException || e instanceof MySQLTransientException || e instanceof SQLRecoverableException|| e instanceof UnableToObtainConnectionException|| (e instanceof SQLException && ((SQLException)e).getErrorCode() == 1317)|| (e instanceof SQLException && shouldRetryException(e.getCause()))|| (e instanceof DBIException && shouldRetryException(e.getCause())));
}
