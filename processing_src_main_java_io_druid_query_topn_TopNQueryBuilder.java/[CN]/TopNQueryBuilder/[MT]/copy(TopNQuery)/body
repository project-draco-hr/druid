{
  return new TopNQueryBuilder().dataSource(query.getDataSource()).dimension(query.getDimensionSpec()).metric(query.getTopNMetricSpec()).threshold(query.getThreshold()).intervals(query.getIntervals()).filters(query.getDimensionsFilter()).granularity(query.getGranularity()).aggregators(query.getAggregatorSpecs()).postAggregators(query.getPostAggregatorSpecs()).context(query.getContext());
}
