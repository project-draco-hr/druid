{
  dataSegment=new DataSegment("",new Interval(0,1),"",null,null,null,new NoneShardSpec(),null,-1);
  localDataSegmentPusher=new LocalDataSegmentPusher(new LocalDataSegmentPusherConfig(),new ObjectMapper());
  dataSegmentFiles=Files.createTempDir();
  ByteStreams.write(Ints.toByteArray(0x9),Files.newOutputStreamSupplier(new File(dataSegmentFiles,"version.bin")));
}
