{
  verifyState();
  final RejectionPolicy rejectionPolicy=rejectionPolicyFactory.create(windowPeriod);
  log.info("Creating plumber using rejectionPolicy[%s]",rejectionPolicy);
  return new RealtimePlumber(schema,config,metrics,emitter,conglomerate,segmentAnnouncer,queryExecutorService,dataSegmentPusher,segmentPublisher,serverView);
}
