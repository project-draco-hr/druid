{
  setupSimple(2);
  Assert.assertEquals(8,supplier.getBaseBuffers().size());
  assertIndexMatchesVals();
  setupSimple(4);
  Assert.assertEquals(4,supplier.getBaseBuffers().size());
  assertIndexMatchesVals();
  setupSimple(32);
  Assert.assertEquals(1,supplier.getBaseBuffers().size());
  assertIndexMatchesVals();
}
