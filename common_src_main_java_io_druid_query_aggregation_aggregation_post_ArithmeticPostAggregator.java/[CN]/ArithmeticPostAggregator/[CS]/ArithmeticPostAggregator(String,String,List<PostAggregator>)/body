{
  this.name=name;
  this.fnName=fnName;
  this.fields=fields;
  if (fields.size() <= 1) {
    throw new IAE("Illegal number of fields[%s], must be > 1",fields.size());
  }
  this.op=Ops.lookup(fnName);
  if (op == null) {
    throw new IAE("Unknown operation[%s], known operations[%s]",fnName,Ops.getFns());
  }
}
