{
  try {
    DatabaseReader reader=new DatabaseReader.Builder(geoDb).withCache(new CHMCache()).build();
    log.info("Using geo ip database at [%s].",geoDb);
    return reader;
  }
 catch (  IOException e) {
    throw new RuntimeException("Could not open geo db at [" + geoDb.getAbsolutePath() + "].",e);
  }
}
