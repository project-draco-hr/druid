{
  int numMinutes=0;
  for (  Interval interval : query.getIntervals()) {
    numMinutes+=Minutes.minutesIn(interval).getMinutes();
  }
  return new ServiceMetricEvent.Builder().setUser2(query.getDataSource()).setUser4(query.getType()).setUser5(Joiner.on(",").join(query.getIntervals())).setUser6(String.valueOf(query.hasFilters())).setUser9(Minutes.minutes(numMinutes).toString()).setUser10(QueryHelper.getQueryId(query));
}
