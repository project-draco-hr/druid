{
  if (!schema.getGranularitySpec().bucketIntervals().isEmpty()) {
    Interval interval=JodaUtils.umbrellaInterval(JodaUtils.condenseIntervals(schema.getGranularitySpec().bucketIntervals()));
    return taskActionClient.submit(new LockTryAcquireAction(interval)).isPresent();
  }
 else {
    return true;
  }
}
