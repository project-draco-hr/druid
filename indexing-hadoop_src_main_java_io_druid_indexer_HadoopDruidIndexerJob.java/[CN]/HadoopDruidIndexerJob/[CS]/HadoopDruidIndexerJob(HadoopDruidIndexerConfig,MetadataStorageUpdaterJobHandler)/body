{
  config.verify();
  this.config=config;
  Preconditions.checkArgument(!config.isUpdaterJobSpecSet() || handler != null,"MetadataStorageUpdaterJobHandler must not be null if ioConfig.metadataUpdateSpec is specified in ");
  if (config.isUpdaterJobSpecSet()) {
    metadataStorageUpdaterJob=new MetadataStorageUpdaterJob(config,handler);
  }
 else {
    metadataStorageUpdaterJob=null;
  }
}
