{
  final Map<String,Object> retMap;
  try {
    final T ret=taskMasterLifecycle.getTaskActionClient(holder.getTask()).submit(holder.getAction());
    retMap=ImmutableMap.<String,Object>of("result",ret);
  }
 catch (  IOException e) {
    return Response.serverError().build();
  }
  return Response.ok().entity(retMap).build();
}
