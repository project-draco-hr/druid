{
  HadoopDruidIndexerConfig config=makeHadoopDruidIndexerConfig(ImmutableMap.<String,Object>of("type","dataSource","ingestionSpec",ImmutableMap.of("dataSource","xyz","interval",interval),"segments",segments),temporaryFolder.newFolder());
