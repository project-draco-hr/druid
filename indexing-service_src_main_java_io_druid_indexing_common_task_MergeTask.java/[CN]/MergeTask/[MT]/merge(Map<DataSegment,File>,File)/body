{
  return IndexMerger.mergeQueryableIndex(Lists.transform(ImmutableList.copyOf(segments.values()),new Function<File,QueryableIndex>(){
    @Override public QueryableIndex apply(    @Nullable File input){
      try {
        return IndexIO.loadIndex(input);
      }
 catch (      Exception e) {
        throw Throwables.propagate(e);
      }
    }
  }
),aggregators.toArray(new AggregatorFactory[aggregators.size()]),outDir,indexSpec);
}
