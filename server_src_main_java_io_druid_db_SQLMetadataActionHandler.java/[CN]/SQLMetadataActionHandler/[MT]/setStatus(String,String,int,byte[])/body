{
  return retryingHandle(new HandleCallback<Integer>(){
    @Override public Integer withHandle(    Handle handle) throws Exception {
      return handle.createStatement(String.format("UPDATE %s SET active = :active, status_payload = :status_payload WHERE id = :id AND active = 1",tableName)).bind("id",Id).bind("active",active).bind("status_payload",statusPayload).execute();
    }
  }
);
}
