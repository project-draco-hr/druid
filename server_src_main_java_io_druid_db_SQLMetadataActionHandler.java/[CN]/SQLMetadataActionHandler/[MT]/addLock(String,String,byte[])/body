{
  return retryingHandle(new HandleCallback<Integer>(){
    @Override public Integer withHandle(    Handle handle) throws Exception {
      return handle.createStatement(String.format("INSERT INTO %s (task_id, lock_payload) VALUES (:task_id, :lock_payload)",tableName)).bind("task_id",Id).bind("lock_payload",lock).execute();
    }
  }
);
}
