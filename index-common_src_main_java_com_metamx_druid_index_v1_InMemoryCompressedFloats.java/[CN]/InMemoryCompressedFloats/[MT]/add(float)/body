{
  int retVal=-1;
  if (!endBuffer.hasRemaining()) {
    endBuffer.rewind();
    compressedBuffers.add(strategy.toBytes(StupidResourceHolder.create(endBuffer)));
    endBuffer=FloatBuffer.allocate(sizePer);
    endBuffer.mark();
    numCompressed+=sizePer;
  }
  retVal=numCompressed + endBuffer.position();
  endBuffer.put(value);
  ++numInserted;
  return retVal;
}
