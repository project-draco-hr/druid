{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  AuditInfo that=(AuditInfo)o;
  if (!author.equals(that.author)) {
    return false;
  }
  if (!comment.equals(that.comment)) {
    return false;
  }
  if (!ip.equals(that.ip)) {
    return false;
  }
  return true;
}
