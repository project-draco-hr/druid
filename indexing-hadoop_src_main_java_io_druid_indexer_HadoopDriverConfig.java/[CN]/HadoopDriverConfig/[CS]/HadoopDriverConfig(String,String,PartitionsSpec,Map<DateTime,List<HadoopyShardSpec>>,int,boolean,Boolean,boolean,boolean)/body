  this.leaveIntermediate=leaveIntermediate;
  this.cleanupOnFailure=(cleanupOnFailure == null ? true : cleanupOnFailure);
  this.overwriteFiles=overwriteFiles;
  this.ignoreInvalidRows=ignoreInvalidRows;
}
