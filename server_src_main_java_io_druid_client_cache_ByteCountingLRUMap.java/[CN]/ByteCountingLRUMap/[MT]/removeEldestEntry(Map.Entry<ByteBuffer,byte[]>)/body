{
  if (numBytes > sizeInBytes) {
    ++evictionCount;
    if (logEvictions && evictionCount % logEvictionCount == 0) {
      log.info("Evicting %,dth element.  Size[%,d], numBytes[%,d], averageSize[%,d]",evictionCount,size(),numBytes,numBytes / size());
    }
    numBytes-=eldest.getKey().remaining() + eldest.getValue().length;
    return true;
  }
  return false;
}
