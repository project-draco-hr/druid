{
  super(String.format("index_%s_%s",dataSource,new DateTime().toString()),dataSource,new Interval(granularitySpec.bucketIntervals().first().getStart(),granularitySpec.bucketIntervals().last().getEnd()));
  this.granularitySpec=Preconditions.checkNotNull(granularitySpec,"granularitySpec");
  this.aggregators=aggregators;
  this.indexGranularity=indexGranularity;
  this.targetPartitionSize=targetPartitionSize;
  this.firehoseFactory=firehoseFactory;
  this.rowFlushBoundary=rowFlushBoundary;
}
