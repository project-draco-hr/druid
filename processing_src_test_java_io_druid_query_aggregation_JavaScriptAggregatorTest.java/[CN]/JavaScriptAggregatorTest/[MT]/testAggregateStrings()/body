{
  final TestObjectColumnSelector ocs=new TestObjectColumnSelector("what",new String[]{"hey","there"});
  final JavaScriptAggregator agg=new JavaScriptAggregator("billy",Collections.<ObjectColumnSelector>singletonList(ocs),JavaScriptAggregatorFactory.compileScript("function aggregate(current, a) { if (typeof a === 'string') { return current + 1; } else { return current + a.length; } }",scriptDoubleSum.get("fnReset"),scriptDoubleSum.get("fnCombine")));
  agg.reset();
  Assert.assertEquals("billy",agg.getName());
  double val=0.;
  Assert.assertEquals(val,agg.get());
  Assert.assertEquals(val,agg.get());
  Assert.assertEquals(val,agg.get());
  aggregate(ocs,agg);
  val+=1;
  Assert.assertEquals(val,agg.get());
  Assert.assertEquals(val,agg.get());
  Assert.assertEquals(val,agg.get());
  aggregate(ocs,agg);
  val+=2;
  Assert.assertEquals(val,agg.get());
  Assert.assertEquals(val,agg.get());
  Assert.assertEquals(val,agg.get());
}
