{
  ArrayList<GenericIndexedWriter<ImmutableBitmap>> writers=Lists.newArrayListWithCapacity(mergedDimensions.size());
  final BitmapSerdeFactory bitmapSerdeFactory=indexSpec.getBitmapSerdeFactory();
  for (  String dimension : mergedDimensions) {
    GenericIndexedWriter<ImmutableBitmap> writer=new GenericIndexedWriter<>(ioPeon,String.format("%s.inverted",dimension),bitmapSerdeFactory.getObjectStrategy());
    writer.open();
    writers.add(writer);
  }
  return writers;
}
