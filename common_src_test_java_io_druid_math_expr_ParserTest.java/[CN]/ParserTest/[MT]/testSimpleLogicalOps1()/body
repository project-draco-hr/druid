{
  String actual=Parser.parse("x>y").toString();
  String expected="(> x y)";
  Assert.assertEquals(expected,actual);
  actual=Parser.parse("x<y").toString();
  expected="(< x y)";
  Assert.assertEquals(expected,actual);
  actual=Parser.parse("x<=y").toString();
  expected="(<= x y)";
  Assert.assertEquals(expected,actual);
  actual=Parser.parse("x>=y").toString();
  expected="(>= x y)";
  Assert.assertEquals(expected,actual);
  actual=Parser.parse("x==y").toString();
  expected="(== x y)";
  Assert.assertEquals(expected,actual);
  actual=Parser.parse("x!=y").toString();
  expected="(!= x y)";
  Assert.assertEquals(expected,actual);
  actual=Parser.parse("x and y").toString();
  expected="(and x y)";
  Assert.assertEquals(expected,actual);
  actual=Parser.parse("x or y").toString();
  expected="(or x y)";
  Assert.assertEquals(expected,actual);
}
