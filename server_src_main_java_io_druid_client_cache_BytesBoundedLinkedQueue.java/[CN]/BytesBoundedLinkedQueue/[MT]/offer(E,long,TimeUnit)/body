{
  checkNotNull(e);
  long nanos=unit.toNanos(timeout);
  boolean added=false;
  putLock.lockInterruptibly();
  try {
    while (currentSize.get() >= capacity) {
      if (nanos <= 0) {
        break;
      }
      nanos=notFull.awaitNanos(nanos);
    }
    delegate.add(e);
    elementAdded(e);
    added=true;
  }
  finally {
    putLock.unlock();
  }
  if (added) {
    signalNotEmpty();
  }
  return added;
}
