{
  checkNotNull(e);
  boolean added=false;
  putLock.lock();
  try {
    if (currentSize.get() >= capacity) {
      return false;
    }
 else {
      added=delegate.add(e);
      if (added) {
        elementAdded(e);
      }
    }
  }
  finally {
    putLock.unlock();
  }
  if (added) {
    signalNotEmpty();
  }
  return added;
}
