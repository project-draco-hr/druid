{
  DateTime now=new DateTime();
  PeriodDropRule rule=new PeriodDropRule(new Period("P5000Y"));
  Assert.assertTrue(rule.appliesTo(builder.interval(new Interval(now.minusDays(2),now.minusDays(1))).build()));
  Assert.assertTrue(rule.appliesTo(builder.interval(new Interval(now.minusYears(100),now.minusDays(1))).build()));
}
