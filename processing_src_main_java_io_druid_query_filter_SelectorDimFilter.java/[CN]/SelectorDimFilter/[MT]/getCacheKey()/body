{
  byte[] dimensionBytes=dimension.getBytes(Charsets.UTF_8);
  byte[] valueBytes=(value == null) ? new byte[]{} : value.getBytes(Charsets.UTF_8);
  return ByteBuffer.allocate(1 + dimensionBytes.length + valueBytes.length).put(DimFilterCacheHelper.SELECTOR_CACHE_ID).put(dimensionBytes).put(valueBytes).array();
}
