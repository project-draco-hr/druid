{
  byte[] dimensionBytes=StringUtils.toUtf8(dimension);
  byte[] valueBytes=(value == null) ? new byte[]{} : StringUtils.toUtf8(value);
  byte[] extractionFnBytes=extractionFn == null ? new byte[0] : extractionFn.getCacheKey();
  return ByteBuffer.allocate(3 + dimensionBytes.length + valueBytes.length+ extractionFnBytes.length).put(DimFilterCacheHelper.SELECTOR_CACHE_ID).put(dimensionBytes).put(DimFilterCacheHelper.STRING_SEPARATOR).put(valueBytes).put(DimFilterCacheHelper.STRING_SEPARATOR).put(extractionFnBytes).array();
}
