{
  if (taskMaster == null) {
    final DruidNode nodeConfig=getConfigFactory().build(DruidNode.class);
    taskMaster=new TaskMaster(taskQueue,taskActionClientFactory,nodeConfig,getZkPaths(),taskRunnerFactory,resourceManagementSchedulerFactory,getCuratorFramework(),serviceAnnouncer,emitter);
    getLifecycle().addManagedInstance(taskMaster);
  }
}
