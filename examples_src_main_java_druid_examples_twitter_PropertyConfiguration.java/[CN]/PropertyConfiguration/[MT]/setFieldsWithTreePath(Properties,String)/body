{
  setFieldsWithPrefix(props,"");
  String[] splitArray=z_InternalStringUtil.split(treePath,"/");
  String prefix=null;
  for (  String split : splitArray) {
    if (!"".equals(split)) {
      if (null == prefix) {
        prefix=split + ".";
      }
 else {
        prefix+=split + ".";
      }
      setFieldsWithPrefix(props,prefix);
    }
  }
}
