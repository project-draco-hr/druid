{
  try {
    httpClient.post(new URL(String.format("http://%s:%s/mmx/merger/v1/index",serviceProvider.getInstance().getAddress(),serviceProvider.getInstance().getPort()))).setContent("application/json",jsonMapper.writeValueAsBytes(killQuery)).go(responseHandler).get();
