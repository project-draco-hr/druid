{
  IntList list=new IntList(5);
  Assert.assertEquals(0,list.length());
  for (int i=0; i < 25; ++i) {
    list.add(i);
    Assert.assertEquals(i + 1,list.length());
  }
  Assert.assertEquals(25,list.length());
  for (int i=0; i < list.length(); ++i) {
    Assert.assertEquals(i,list.get(i));
  }
}
