{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  PagingSpec that=(PagingSpec)o;
  if (fromNext != that.fromNext) {
    return false;
  }
  if (threshold != that.threshold) {
    return false;
  }
  if (!pagingIdentifiers.equals(that.pagingIdentifiers)) {
    return false;
  }
  return true;
}
