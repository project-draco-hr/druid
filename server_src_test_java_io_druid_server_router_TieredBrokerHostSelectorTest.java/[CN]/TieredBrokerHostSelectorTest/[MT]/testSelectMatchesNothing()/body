{
  String brokerName=(String)brokerSelector.select(Druids.newTimeseriesQueryBuilder().dataSource("test").granularity("all").aggregators(Arrays.<AggregatorFactory>asList(new CountAggregatorFactory("rows",null))).intervals(Arrays.<Interval>asList(new Interval("2010-08-31/2010-09-01"))).build()).lhs;
  Assert.assertEquals("hotBroker",brokerName);
}
