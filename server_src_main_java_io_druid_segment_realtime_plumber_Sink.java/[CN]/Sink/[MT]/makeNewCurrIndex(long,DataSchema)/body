{
  IncrementalIndex newIndex=new IncrementalIndex(new IncrementalIndexSchema.Builder().withMinTimestamp(minTimestamp).withQueryGranularity(schema.getGranularitySpec().getQueryGranularity()).withSpatialDimensions(schema.getParser()).withMetrics(schema.getAggregators()).build());
  final FireHydrant old;
synchronized (hydrantLock) {
    old=currHydrant;
    currHydrant=new FireHydrant(newIndex,hydrants.size(),getSegment().getIdentifier());
    hydrants.add(currHydrant);
  }
  return old;
}
