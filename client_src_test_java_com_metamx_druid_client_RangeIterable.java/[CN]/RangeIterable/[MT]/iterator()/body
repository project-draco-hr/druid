{
  return new Iterator<Integer>(){
    int value=startValue;
    @Override public boolean hasNext(){
      return value < endValue;
    }
    @Override public Integer next(){
      try {
        return value;
      }
  finally {
        value+=increment;
      }
    }
    @Override public void remove(){
      throw new UnsupportedOperationException();
    }
  }
;
}
