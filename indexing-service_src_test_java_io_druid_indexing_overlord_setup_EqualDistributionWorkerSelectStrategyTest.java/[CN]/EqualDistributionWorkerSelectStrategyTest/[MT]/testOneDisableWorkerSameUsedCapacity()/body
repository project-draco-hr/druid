{
  String DISABLED_VERSION="";
  final EqualDistributionWorkerSelectStrategy strategy=new EqualDistributionWorkerSelectStrategy();
  Optional<ImmutableZkWorker> optional=strategy.findWorkerForTask(new RemoteTaskRunnerConfig(),ImmutableMap.of("lhost",new ImmutableZkWorker(new Worker("disableHost","disableHost",10,DISABLED_VERSION,DateTime.now()),5,Sets.<String>newHashSet()),"localhost",new ImmutableZkWorker(new Worker("enableHost","enableHost",10,"v1",DateTime.now()),5,Sets.<String>newHashSet())),new NoopTask(null,1,0,null,null,null){
    @Override public String getDataSource(){
      return "foo";
    }
  }
);
  ImmutableZkWorker worker=optional.get();
  Assert.assertEquals("enableHost",worker.getWorker().getHost());
}
