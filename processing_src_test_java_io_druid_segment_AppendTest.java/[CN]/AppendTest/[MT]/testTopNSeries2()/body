{
  List<Result<TopNResultValue>> expectedResults=Arrays.asList(new Result<TopNResultValue>(new DateTime("2011-01-12T00:00:00.000Z"),new TopNResultValue(Arrays.<Map<String,Object>>asList(ImmutableMap.<String,Object>builder().put("provider","total_market").put("rows",3L).put("index",300.0D).put("addRowsIndexConstant",304.0D).put("uniques",0.0D).put("maxIndex",100.0D).put("minIndex",100.0D).build(),new HashMap<String,Object>(){
