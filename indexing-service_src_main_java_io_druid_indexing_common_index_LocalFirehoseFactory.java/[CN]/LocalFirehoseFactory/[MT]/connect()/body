{
  return new FileIteratingFirehose<File>(Lists.<File>newLinkedList(Arrays.<File>asList(baseDir.listFiles(new FilenameFilter(){
    @Override public boolean accept(    File file,    String name){
      return name.contains(filter);
    }
  }
))),parser){
    @Override public LineIterator makeLineIterator(    File file) throws Exception {
      return FileUtils.lineIterator(file);
    }
  }
;
}
