{
  final IncrementalIndex rtIndex=TestIndex.getIncrementalTestIndex();
  final QueryableIndex mMappedTestIndex=TestIndex.getMMappedTestIndex();
  final QueryableIndex mergedRealtimeIndex=TestIndex.mergedRealtimeIndex();
  return Arrays.asList(new Object[][]{{makeQueryRunner(factory,new IncrementalIndexSegment(rtIndex,segmentId))},{makeQueryRunner(factory,new QueryableIndexSegment(segmentId,mMappedTestIndex))},{makeQueryRunner(factory,new QueryableIndexSegment(segmentId,mergedRealtimeIndex))}});
}
