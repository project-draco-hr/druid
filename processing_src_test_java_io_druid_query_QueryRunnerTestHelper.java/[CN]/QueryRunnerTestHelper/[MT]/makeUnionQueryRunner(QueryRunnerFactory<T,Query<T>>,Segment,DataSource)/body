{
  return new FinalizeResultsQueryRunner<T>(factory.getToolchest().postMergeQueryDecoration(factory.getToolchest().mergeResults(new UnionQueryRunner<T>(Iterables.transform(unionDataSource.getNames(),new Function<String,QueryRunner>(){
    @Nullable @Override public QueryRunner apply(    @Nullable String input){
      return new BySegmentQueryRunner<T>(segmentId,adapter.getDataInterval().getStart(),factory.createRunner(adapter));
    }
  }
),factory.getToolchest()))),factory.getToolchest());
}
