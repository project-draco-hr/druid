{
  return dbi.withHandle(new HandleCallback<List<String>>(){
    @Override public List<String> withHandle(    Handle handle) throws Exception {
      final List<Map<String,Object>> dbTasks=handle.createQuery(String.format("SELECT id FROM %s WHERE status_code = :status_code",dbConnectorConfig.getTaskTable())).bind("status_code",TaskStatus.Status.RUNNING.toString()).list();
      return Lists.transform(dbTasks,new Function<Map<String,Object>,String>(){
        @Override public String apply(        Map<String,Object> row){
          return row.get("id").toString();
        }
      }
);
    }
  }
);
}
