{
  final DateTime baseTime=new DateTime("2011-01-01T09:00:00.000Z");
  assertSame(Lists.newArrayList(new DateTime("2011-01-01T09:00:00.000Z"),new DateTime("2011-01-01T10:00:00.000Z"),new DateTime("2011-01-01T11:00:00.000Z")),QueryGranularity.HOUR.iterable(baseTime.getMillis(),baseTime.plus(Hours.hours(3)).getMillis()));
}
