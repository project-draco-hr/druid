{
  final String desiredPrefix="merge_foo_" + Hashing.sha1().hashString("2012-01-03T00:00:00.000Z_2012-01-05T00:00:00.000Z_V1_0" + "_2012-01-04T00:00:00.000Z_2012-01-06T00:00:00.000Z_V1_0" + "_2012-01-05T00:00:00.000Z_2012-01-07T00:00:00.000Z_V1_0",Charsets.UTF_8).toString().toLowerCase() + "_";
  Assert.assertEquals(desiredPrefix,testMergeTask.getId().substring(0,desiredPrefix.length()));
}
