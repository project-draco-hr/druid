{
  int numMinutes=0;
  for (  Interval interval : query.getIntervals()) {
    numMinutes+=Minutes.minutesIn(interval).getMinutes();
  }
  return new ServiceMetricEvent.Builder().setUser2(query.getDataSource().getMetricName()).setUser4("Select").setUser5(COMMA_JOIN.join(query.getIntervals())).setUser6(String.valueOf(query.hasFilters())).setUser9(Minutes.minutes(numMinutes).toString());
}
