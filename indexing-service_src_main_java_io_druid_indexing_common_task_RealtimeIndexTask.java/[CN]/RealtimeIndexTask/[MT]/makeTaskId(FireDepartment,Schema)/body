{
  if (fireDepartment == null) {
    return String.format("index_realtime_%s_%d_%s",schema.getDataSource(),schema.getShardSpec().getPartitionNum(),new DateTime().toString());
  }
 else {
    return String.format("index_realtime_%s_%d_%s",fireDepartment.getDataSchema().getDataSource(),fireDepartment.getDriverConfig().getShardSpec().getPartitionNum(),new DateTime().toString());
  }
}
