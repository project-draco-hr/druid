{
  return Initialization.makeInjector(new LifecycleModule(),EmitterModule.class,HttpClientModule.global(),CuratorModule.class,new MetricsModule(),new ServerModule(),new JettyServerModule(new QueryJettyServerInitializer()).addResource(StatusResource.class).addResource(ChatHandlerResource.class),new DiscoveryModule(),new ServerViewModule(),new StorageNodeModule("real-time"),new DataSegmentPusherModule(),new AnnouncerModule(),new DruidProcessingModule(),new QueryableModule(ThreadPoolTaskRunner.class),new QueryRunnerFactoryModule(),new IndexingServiceDiscoveryModule(),new AWSModule(),new PeonModule());
}
