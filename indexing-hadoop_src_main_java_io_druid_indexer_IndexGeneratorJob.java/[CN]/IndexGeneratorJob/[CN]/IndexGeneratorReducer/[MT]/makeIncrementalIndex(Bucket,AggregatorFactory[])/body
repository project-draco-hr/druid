{
  List<SpatialDimensionSchema> spatialDimensionSchemas=config.getSchema().getDataSchema().getParser() == null ? Lists.<SpatialDimensionSchema>newArrayList() : config.getSchema().getDataSchema().getParser().getParseSpec().getDimensionsSpec().getSpatialDimensions();
  return new IncrementalIndex(new IncrementalIndexSchema.Builder().withMinTimestamp(theBucket.time.getMillis()).withSpatialDimensions(spatialDimensionSchemas).withQueryGranularity(config.getSchema().getDataSchema().getGranularitySpec().getQueryGranularity()).withMetrics(aggs).build());
}
