{
  DruidBinders.dataSegmentPullerBinder(binder).addBinding("s3_zip").to(S3DataSegmentPuller.class).in(LazySingleton.class);
  PolyBind.optionBinder(binder,Key.get(DataSegmentPusher.class)).addBinding("s3").to(S3DataSegmentPusher.class).in(LazySingleton.class);
  JsonConfigProvider.bind(binder,"druid.storage",S3DataSegmentPusherConfig.class);
}
