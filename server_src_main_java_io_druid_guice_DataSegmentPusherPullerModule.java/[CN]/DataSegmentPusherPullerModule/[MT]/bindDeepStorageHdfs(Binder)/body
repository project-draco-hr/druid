{
  DruidBinders.dataSegmentPullerBinder(binder).addBinding("hdfs").to(HdfsDataSegmentPuller.class).in(LazySingleton.class);
  binder.bind(Configuration.class).toInstance(new Configuration());
  PolyBind.optionBinder(binder,Key.get(DataSegmentPusher.class)).addBinding("hdfs").to(HdfsDataSegmentPusher.class).in(LazySingleton.class);
  JsonConfigProvider.bind(binder,"druid.storage",HdfsDataSegmentPusherConfig.class);
}
