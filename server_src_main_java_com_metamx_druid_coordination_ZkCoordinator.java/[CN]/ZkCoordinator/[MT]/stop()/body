{
  log.info("Stopping ZkCoordinator with config[%s]",config);
synchronized (lock) {
    if (!started) {
      return;
    }
    try {
      loadQueueCache.close();
      curator.delete().guaranteed().forPath(ZKPaths.makePath(config.getAnnounceLocation(),me.getName()));
    }
 catch (    Exception e) {
      throw Throwables.propagate(e);
    }
 finally {
      loadQueueCache=null;
      started=false;
    }
  }
}
