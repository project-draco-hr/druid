{
synchronized (this) {
    try {
      hasStarted=true;
      start.countDown();
      neverRelease.lockInterruptibly();
    }
 catch (    InterruptedException e) {
      interrupted=true;
      interruptedRunners.offer(this);
      stop.countDown();
      throw new QueryInterruptedException("I got killed");
    }
  }
  hasCompleted=true;
  stop.countDown();
  return Sequences.simple(Lists.newArrayList(123));
}
