{
  TestUtil.MAPPER.setInjectableValues(new InjectableValues(){
    @Override public Object findInjectableValue(    Object valueId,    DeserializationContext ctxt,    BeanProperty forProperty,    Object beanInstance){
      if (valueId.equals("com.fasterxml.jackson.databind.ObjectMapper")) {
        return TestUtil.MAPPER;
      }
      throw new ISE("No Injectable value found");
    }
  }
);
}
