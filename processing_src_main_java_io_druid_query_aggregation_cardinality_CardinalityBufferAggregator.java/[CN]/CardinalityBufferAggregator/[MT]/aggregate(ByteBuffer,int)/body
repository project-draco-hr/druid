{
  final HyperLogLogCollector collector=HyperLogLogCollector.makeCollector((ByteBuffer)buf.duplicate().position(position).limit(position + HyperLogLogCollector.getLatestNumBytesForDenseStorage()));
  if (byRow) {
    CardinalityAggregator.hashRow(selectorList,collector);
  }
 else {
    CardinalityAggregator.hashValues(selectorList,collector);
  }
}
