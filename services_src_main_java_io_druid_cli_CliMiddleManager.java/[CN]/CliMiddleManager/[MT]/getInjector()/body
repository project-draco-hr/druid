{
  return Initialization.makeInjector(new LifecycleModule().register(WorkerTaskMonitor.class),EmitterModule.class,HttpClientModule.global(),CuratorModule.class,new MetricsModule(),ServerModule.class,new JettyServerModule(new MiddleManagerJettyServerInitializer()).addResource(StatusResource.class).addResource(WorkerResource.class),new AWSModule(),new TaskLogsModule(),new MiddleManagerModule());
