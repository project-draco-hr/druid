{
  Preconditions.checkState(conglomerate == null,"Registering a QueryRunnerFactory only works when a separate conglomerate is not specified.");
  Preconditions.checkState(!additionalFactories.containsKey(queryClazz),"Registered factory for class[%s] multiple times",queryClazz);
  additionalFactories.put(queryClazz,factory);
  return (T)this;
}
