{
  final BasicDataSource datasource=new BasicDataSource();
  datasource.setUrl(connectionURI);
  datasource.setDriverClassLoader(JDBCExtractionNamespaceTest.class.getClassLoader());
  datasource.setDriverClassName("org.apache.derby.jdbc.EmbeddedDriver");
  dbi=new DBI(datasource);
  dbi.withHandle(new HandleCallback<Void>(){
    @Override public Void withHandle(    Handle handle) throws Exception {
      handle.createStatement(String.format("CREATE TABLE %s (%s TIMESTAMP, %s VARCHAR(64), %s VARCHAR(64))",tableName,tsColumn_,keyName,valName)).execute();
      return null;
    }
  }
);
}
